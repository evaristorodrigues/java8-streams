/**
 * 
 */
package java8;

import java.util.stream.IntStream;

/**
 * @author evaristosrodrigues
 *
 */
public class StreamsExamples {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// 1 - Integer Stream
		IntStream
		.range(1, 10)
		.forEach(System.out::print);
		System.out.println();

	}

}
